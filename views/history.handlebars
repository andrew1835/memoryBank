<div class="container historyContainer">
  <h1 id="history-header">History Page</h1>
  <!-- #each to create new row and column displaying date -->
  <div class="row history-row">
    <div class="column s12">
      <div class="historydate">
        {{#each histories}}
        <p>{{lucky_number}}</p>

        {{/each}}
        <a href="#!">inject date {{histories.lucky_number}}</a>
      </div>
    </div>
  </div>
  <div class="row history-row">
    <div class="column s12">
      <div class="historydate">
        <p>1/27/2021</p>
      </div>
    </div>
  </div>
  <div class="row history-row">
    <div class="column s12">
      <div class="historydate">
        <p>1/26/2021</p>
      </div>
    </div>
  </div>
</div>